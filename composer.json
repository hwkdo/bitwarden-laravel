{
    "name": "hwkdo/bitwarden-laravel",
    "description": "This is my package bitwarden-laravel",
    "keywords": [
        "hwkdo",
        "laravel",
        "bitwarden-laravel"
    ],
    "homepage": "https://github.com/hwkdo/bitwarden-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "hwkdo",
            "email": "doit@hwk-do.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Hwkdo\\BitwardenLaravel\\": "src/",
            "Hwkdo\\BitwardenLaravel\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hwkdo\\BitwardenLaravel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hwkdo\\BitwardenLaravel\\BitwardenLaravelServiceProvider"
            ],
            "aliases": {
                "BitwardenLaravel": "Hwkdo\\BitwardenLaravel\\Facades\\BitwardenLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}